{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**/*.{js,html,css,png,jpg,svg,json}",
            "use": "@vercel/static"
        }
    ],
    "rewrites": [
        
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}